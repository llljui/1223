body{margin: 0;overflow-x: hidden;}
.goback{position: fixed;top: 40%;cursor: pointer;opacity: 0.3;}
@media screen and (max-width: 1921px) {
.page{overflow-x:hidden;width:1920px;}

.page_img{overflow-x:hidden;width:100%;}
.p1{position:absolute;top:-11%;left: 31%;opacity: 0;transform:scale(0.0);}
.money{position: absolute;top: 0;}
/* .cpu{position:absolute;} */
.cpu1{position: absolute;top: 212%;left: 50%;}
.cpu2{position: absolute;top:219%;left: 49%;}
.cpu3{position: absolute;top:230%;left: 49%;cursor: pointer;}
.cpu3:hover{transform: scale(1.1, 1.1);transition: 0.5s;}
.cpu_input1{position: absolute;top: 215%;left: 59.5%;z-index: 444;display: block;width:7.6%;height: 5.1%;background: #fe4d3f;border: none;}
.cpu_input1:focus{outline:none}
.cpu_input2{position: absolute;top:221.5%;left: 59.5%;z-index: 666;display: block;width:7.6%;height: 5.1%;background: #fe4d3f;border: none;}
.cpu_input2:focus{outline:none}
.explem{position: absolute;top: 211%;left:47%;}
.myget{position: absolute;top: 238%;left:55%;}
#share li{float: left;list-style: none;position: absolute;top: 303%;width: 10%;}
#share li img{cursor: pointer;}
#share li img:hover{transition: 0.5s;transform: scale(1.1,1.1);}
.weixin{left:27.5%;}
.qq{left: 39.5%;}
.weibo{left: 52%;}
.copy{left:64.5%;top: 304%!important;}
}

