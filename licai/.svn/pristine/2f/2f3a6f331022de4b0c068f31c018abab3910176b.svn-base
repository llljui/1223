.cpu_input1{outline: none;background:url('../img/input.png') no-repeat;display:block;width:100px;height:30px;background-size:280% 280%;background-position:50% 50%;border-radius: 5px;border:0;}
.cpu_input2{outline: none;background:url('../img/input.png') no-repeat;display:block;width:100px;height:30px;background-size:280% 280%;background-position:50% 50%;border-radius: 5px;border:0;}
.p3_3{margin-left:15px;top:1190px;width:275px;cursor: pointer;}
.p3_3:hover{transform: scale(1.1, 1.1);transition: 0.5s;}
.explem{margin:10px 20px;position: relative;top: -324px;left: 510px;}
.myget{margin:10px 20px;position: relative;top: -200px;left: 650px;}
.page4{width: 1200px;margin:0 auto;margin-top:-215px;}
.share{width:580px;height:95px;/* border: 1px solid red; */position: relative;top:-150px;left:280px;}
.share div{float: left;width: 100px;height: 100px;margin-left: 38px;cursor: pointer;}
.page5{width: 1200px;margin:0 auto;margin-top:-100px;clear: both;}
.p5_5{margin-top: -105px;}
.fr_m{display: inline-block;margin-left:63px; margin-bottom: 10px;margin-right: 10px;font-weight: 900;}
.fr_t{display: inline-block;margin-left:69px;margin-right: 10px;}
#share-1{margin-left: 45px;}
.help{width: 180px!important;text-align: center;margin-left: 5px!important;}
.qrcode{margin-bottom:10px!important;margin-left: 38px!important;}
#share-4{margin-left: 53px;}
