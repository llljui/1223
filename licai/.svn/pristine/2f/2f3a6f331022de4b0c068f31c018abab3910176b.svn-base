body{margin: 0;overflow-x: hidden;}
.goback{position: fixed;top: 40%;cursor: pointer;opacity: 0.3;}
.page1,.page2,.page3,.page4.page5{width:1200px;margin:0 auto;}
.p1{position:absolute;top:0%;left:38%;opacity: 0;transform:scale(0.0);z-index: 5;width: 400px;}
.money{position: absolute;top:-100px;opacity: 1;}
.page2,.page3,.page4,.page5{margin-top:-5px;}
