*{margin:0;overflow-x: hidden;list-style: none;}

@media screen and (min-width: 1900px){
body{-webkit-perspective: 300px;perspective: 300px;}
.all_page{width: 1920px;margin: 0 auto;}
.car_y{position: absolute;margin-top:-1085px;margin-left: 330px;width:250px;}
.car_g{position: absolute;margin-top:550px;margin-left:975px;}
.act_time,.boat,.handle,.m_580{position: absolute;}
.boat{margin-top:-940px;margin-left:1035px;-webkit-transform:scale(0.2, 0.2);transform:scale(0.2, 0.2);opacity: 0;width:260px;}
.handle{margin-top:-1820px;margin-left:670px;opacity: 0;-webkit-transform: translate3d(200px, 0, 200px);transform: translate3d(200px, 0, 200px);width:770px;}
.m_580{margin-top:-710px;margin-left:235px;width:450px;}
.act_time{margin-top:-610px;margin-left:1100px;opacity: 0;width:255px;}
.ecv{position: absolute;margin-top:100px;}
.page1,.page2,.page3,.page4,.page5,.page6,.page7,.page8{margin:0 auto;width: 100%;margin-top: -18px}
.page4{margin-top: -11px;}
.bao{position: absolute;margin-top: -205px;margin-left: 335px;cursor: pointer;width: 15%;}
.bao1{-webkit-transform: rotateY(180deg);transform: rotateY(180deg);}
.bao2{margin-top: -100px;margin-left: 1225px;}
.bao3{-webkit-transform: rotateY(180deg);transform: rotateY(180deg);margin-top: -235px;margin-left: 585px;}
.bao4{margin-top: -365px;margin-left: 1345px;}
.bao5{-webkit-transform: rotateY(180deg);transform: rotateY(180deg);margin-left: 295px;margin-top: -225px;}
.bao6{margin-top: -670px;margin-left: 1295px;}
.h_1{height:650px;width: 100%;background:#a3f2fe;z-index: -6;position: absolute;}
.h_1_1{position: absolute;width: 50%;height: 115px;background:/* url('../img/h_1_1.png') */#a3f2fe;margin-top:650px;z-index:-1;background-position:100% 100%;}
.h_1_2{position: absolute;right: 0;width: 50%;height: 120px;background:/* url('../img/h_1_1.png') */#a3f2fe;margin-top:642px;z-index:-1;background-position:100% 100%;}
.h_1_3{position: absolute;width: 50%;height: 115px;background:/* url('../img/h_1_1.png') */#ffc41e;margin-top:734px;z-index:-1;background-position:100% 100%;}
.h_1_4{position: absolute;width: 50%;height: 115px;background:/* url('../img/h_1_1.png') */#ffc41e;margin-top:758px;z-index:-1;background-position:100% 100%;right: 0;}
.h_2_1{position: absolute;width: 100%;height: 2870px;background:#fdeb83;margin-top:849px;z-index:-1;}
}
